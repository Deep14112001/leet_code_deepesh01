int min(int a,int b){
        if(a<b) return a;
        return b;
    }
    int maxArea(vector<int>& height) {
        int sum=0;
        for(int i=height.size()-1;i>0;i--){
            for(int j=0;j<i;j++){
                int sum1=(i-j)*min(height[i],height[j]);
                if(sum1>sum) sum=sum1;
                if(height[i]*(i-j)<sum){
                    break;
                }
            }
        }
        return sum;
    }
