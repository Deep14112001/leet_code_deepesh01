 int maximum69Number (int num) {
        vector<int> yours;
        while(num!=0){
            yours.push_back(num%10);
            num/=10;
        }
        for(int i=yours.size()-1;i>=0;i--){
            if(yours[i]==6){
                yours[i]=9;
                break;
            }
        }
        int ans=0;
        int b=1;
        for(int j=0;j<yours.size();j++){
            ans=ans+yours[j]*b;
            b*=10;
        }
        return ans;
